{
  "name": "jsninja-jwt-demo",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "test": "better-npm-run test",
    "lint": "eslint src tests",
    "precommit": "lint-staged"
  },
  "betterScripts": {
    "test": {
      "command": "ava",
      "env": {
        "JWT_connection": "./tests/__data",
        "JWT_secret": "TEST"
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "keywords": [],
  "author": "Illya Klymov <illya@javascript.ninja",
  "license": "MIT",
  "devDependencies": {
    "ava": "^3.11.1",
    "better-npm-run": "^0.1.1",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "supertest-koa-agent": "^0.3.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-jwt": "^4.0.0",
    "koa-router": "^9.4.0",
    "lodash": "^4.17.20",
    "rc": "^1.2.8",
    "uuid": "^8.3.0"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "env": {
      "node": true
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": true
  },
  "ava": {
    "files": [
      "tests/**/*.js",
      "!tests/__data/**/*.js",
      "!tests/helpers/**/*.js"
    ]
  }
}
